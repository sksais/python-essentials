   "source": [
    "for number in range(1042000,702648265):\n",
    "    order = len(str(num))\n",
    "    sum = 0\n",
    "    temp = number\n",
    "    while temp > 0:\n",
    "       digit = temp % 10\n",
    "       sum += digit ** order\n",
    "       temp //= 10\n",
    "\n",
    "    if number == sum:\n",
    "        print(number)\n",
    "        break\n",
    "   \n",
    "       \n",
    "           \n"
   ]
